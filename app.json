{
  "expo": {
    "name": "Staked",
    "slug": "staked",
    "scheme": "staked",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAF9F7"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.expocharles.staked",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to update your profile picture.",
        "NSPhotoLibraryUsageDescription": "This app uses your photo library to select a profile picture."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FAF9F7"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.expocharles.staked",
      "versionCode": 1,
      "permissions": [
        "NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#FF6F00",
          "sounds": []
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "993ffa72-a003-4ee8-a03a-d0e9fae91db4"
      }
    },
    "owner": "expocharles"
  }
}
